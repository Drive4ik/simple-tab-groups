{
    "name": "simple-tab-groups",
    "version": "1.0.0",
    "description": "Create, modify and quickly change tab groups. \nInspired by the old add-on \"Tab Groups\" :)",
    "author": "Drive4ik <drive4ik@gmail.com>",
    "scripts": {
        "build": "webpack --config webpack.config.js --mode development --progress --hide-modules",
        "build:prod": "webpack --config webpack.config.js --mode production --progress --hide-modules",
        "watch": "npm run build -- --watch",
        "watch:prod": "npm run build:prod -- --watch",
        "build-zip": "node scripts/build-zip.js"
    },
    "dependencies": {
        "vue": "^2.5.16",
        "vue-swatches": "^1.0.1"
    },
    "devDependencies": {
        "@babel/core": "^7.0.0-beta.49",
        "@babel/preset-env": "^7.0.0-beta.49",
        "babel-loader": "^8.0.0-beta.3",
        "copy-webpack-plugin": "^4.0.1",
        "css-loader": "^0.28.11",
        "extract-text-webpack-plugin": "^4.0.0-beta.0",
        "file-loader": "^1.1.11",
        "html-webpack-plugin": "^3.2.0",
        "node-sass": "^4.9.0",
        "sass-loader": "^7.0.1",
        "vue-loader": "^15.2.2",
        "vue-template-compiler": "^2.5.13",
        "webpack": "^4.10.1",
        "webpack-cli": "^2.1.4",
        "webpack-shell-plugin": "^0.5.0",
        "zip-folder": "^1.0.0"
    }
